{
  "version": "Notebook/1.0",
  "items": [
    {
      "type": 1,
      "content": {
        "json": "# IaaP Evidence Overview"
      }
    },
    {
      "type": 3,
      "content": {
        "version": "TimeRangeItem/1.0",
        "parameters": {
          "timeRange": {
            "name": "timeRangeParam",
            "type": "TimeRange",
            "isRequired": true,
            "value": {
              "durationMs": 3600000
            }
          }
        }
      }
    },
    {
      "type": 7,
      "content": {
        "version": "ParameterItem/1.0",
        "parameters": [
          {
            "name": "kindParam",
            "type": "text",
            "label": "Kind filter",
            "value": ""
          },
          {
            "name": "statusParam",
            "type": "text",
            "label": "Status filter",
            "value": ""
          }
        ]
      }
    },
    {
      "type": 9,
      "content": {
        "version": "KqlItem/1.0",
        "query": "IaapInfraEvidence_CL | where (isempty('{kindParam}') or kind_s == '{kindParam}') | where (isempty('{statusParam}') or status_s == '{statusParam}') | where TimeGenerated between ( {timeRangeParam:start} .. {timeRangeParam:end} ) | order by TimeGenerated desc | take 200",
        "size": 1
      }
    }
  ]
}